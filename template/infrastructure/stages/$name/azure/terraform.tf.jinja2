
terraform {
  backend "azurerm" {
    resource_group_name  = "{{ }}"
    storage_account_name = "{{ }}"
    container_name       = "tfstate"
    key                  = "{{ }}" # refers to the file name
  }
}



terraform {
  required_version = ">= 1.5.0"

  required_providers {

    azurerm = {
      source  = "hashicorp/azurerm"
      version = ">= 3.63.0"
    }

    azuread = {
      source  = "hashicorp/azuread"
      version = ">= 2.39.0"
    }
  }
}