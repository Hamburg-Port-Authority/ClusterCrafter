{
    "$id": "https://hpa.hamburg.de/observability/victoria-metrics-cluster/configuration.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Observability Victoria Metrics Cluster Configuration Schema",
    "type": "object",
    "properties": {
        "defaults": {
            "type": "object",
            "$ref": "../defaults.json"
        },
        "replicationFactor": {
            "type": "integer",
            "default": "2"
        },
        "vmstorage": {
