{
    "$id": "https://hpa.hamburg.de/observability/victoria-metrics-alertmanager/configuration.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Observability Victoria Metrics Alertmanager Configuration Schema",
    "type": "object",
    "properties": {
        "defaults": {
            "type": "object",
            "$ref": "../defaults.json"
        },
        "imageRepository": {
            "type": "string",
            "default": "victoriametrics/vmagent"
        },
        "ingress": {
            "type": "object",
            "$ref": "../ingress.json"
        }
    },
    "required": [
